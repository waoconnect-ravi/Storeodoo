<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="view_order_form_for_nzd" model="ir.ui.view">
        <field name="name">View Order Form For nzd</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[last()]" position="after">
                <page string="NZD Information" name="nzd_information">
                    <group>
                        <group>
                            <field name="send_order_to_nzd" readonly="0"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
