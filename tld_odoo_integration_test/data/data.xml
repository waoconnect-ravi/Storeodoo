<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!--Create a tld shipping product-->
    <record id="tld_shipping_product" model="product.product">
        <field name="name">TLD Shipping Product</field>
        <field name="type">service</field>
        <field name="default_code">tldshippingproduct</field>
        <field name="invoice_policy">order</field>
    </record>

    <record id="tld_validate_delivery_orders" model="ir.cron">
        <field name="name">TLD Validate Delivery orders</field>
        <field name="model_id" ref="model_sftp_syncing"/>
        <field name="state">code</field>
        <field name="code">model.tld_validate_delivery_orders()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>

    <record id="tld_validate_purchase_orders" model="ir.cron">
        <field name="name">tld Validate purchase orders</field>
        <field name="model_id" ref="model_sftp_syncing"/>
        <field name="state">code</field>
        <field name="code">model.tld_validate_purchase_orders()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>

    <record id="tld_purchase_order_despatch_report_fetch" model="ir.cron">
        <field name="name">tld purchase order despatch report fetch</field>
        <field name="model_id" ref="model_sftp_syncing"/>
        <field name="state">code</field>
        <field name="code">model.tld_purchase_order_despatch_report_fetch()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>
</odoo>
